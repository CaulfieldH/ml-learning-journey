{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "python": "${workspaceFolder}/ml_env/Scripts/python.exe",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "Python: ML Script",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/netology_ml_l8.py",
            "console": "integratedTerminal",
            "python": "${workspaceFolder}/ml_env/Scripts/python.exe"
        }
    ]
}